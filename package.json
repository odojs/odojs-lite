{
  "name": "odojs.com",
  "private": true,
  "version": "1.0.0",
  "description": "The odojs.com website written in odo.js",
  "main": "index.js",
  "scripts": {
    "js": "./node_modules/.bin/browserify ./index.js --debug | ./node_modules/.bin/exorcist ./bundle.js.map > ./bundle.js",
    "wjs": "./node_modules/.bin/watchify ./index.js -o ./bundle.js",
    "css": "./node_modules/.bin/stylus --include-css ./index.styl -o ./bundle.css",
    "wcss": "./node_modules/.bin/stylus --include-css --watch ./index.styl -o ./bundle.css"
  },
  "author": "Thomas Coats <github@voodoolabs.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/odojs/odojs-lite.git"
  },
  "bugs": {
    "url": "https://github.com/odojs/odojs-lite/issues"
  },
  "homepage": "https://github.com/odojs/odojs-lite",
  "dependencies": {
    "express": "4.13.4",
    "http-mutunga": "1.0.0",
    "injectinto": "1.2.0",
    "marked": "0.3.5",
    "odo-relay": "2.4.0",
    "odo-route": "1.3.1",
    "odojs": "4.4.0",
    "odoql": "7.10.0",
    "odoql-exe": "1.6.2",
    "odoql-store": "0.1.0",
    "page": "1.7.1",
    "superagent": "1.8.1",
    "whakaruru": "1.0.2"
  },
  "devDependencies": {
    "exorcist": "0.4.0",
    "browserify": "13.0.0",
    "stylus": "0.54.2",
    "watchify": "3.7.0"
  }
}
